<h1>New Article</h1>

<%= form_with model: @article do |form| %>
  <div>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :body %><br>
    <%= form.text_area :body %>
  </div>

   <div>
    <%= form.label :profile_image %><br>
    <%= form.file_field :profile_image %>
  </div>

  <div>
    <%= form.submit "Submit" %>
  </div>
<% end %>